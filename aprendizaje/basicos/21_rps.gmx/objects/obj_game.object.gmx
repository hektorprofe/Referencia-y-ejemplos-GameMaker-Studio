            <kind>1</kind>
            <string>/// Timer and trigger GAME CHECK + IA response
if (checking == true) {
    if (!is_string(timer_seconds)){
        if ( timer_seconds &gt; 0 ) {
            timer_seconds -= 1;
            
            // Move hands to center when seconds == 2
            if (timer_seconds == 0) {
                with(obj_hand) move_to_center = true;
                obj_cross.move_to_center = true;
            }
            
            alarm[0] = room_speed; 
            if (timer_seconds == 0) {
            
                timer_seconds = "GO!"; 
        
                // Get IA response
                ia_get_play();
            }
        }
    } else {
        checking = false; 
        
        // Check winner
        var res = check_winner();
        
        // Show result
        if (res == -1) {
            final_result = "It's a draw!";
        } else if (res == 0) {
            final_result = "You lose!";
            global.p2_score++;
        } else {
            final_result = "You win!";
            global.p1_score++;
        }
        
        // After X seconds restart room to rematch
        alarm[1] = room_speed;
    }
}

</string>
