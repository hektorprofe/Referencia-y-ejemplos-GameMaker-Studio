            <string>/// Send PING every 3 seconds

if (connected == true) {
    var ping_packet = buffer_create(1, buffer_grow, 1);
    buffer_write(ping_packet, buffer_string, "ping");
    network_write(network.socket, ping_packet);
    pings+=1;
    
    // Compruebo si hay m√°s de 3 pings de retraso = 10s
    //show_debug_message(string(pings) + " " + string(pongs));
    if (pings&gt;pongs+1) connected = false;
    
    // A los 3 segundos envio el ping y compruebo otra vez
    alarm[1] = room_speed * 3;
} else {
    // Close the socket to be efficient
    if instance_exists(network){
        with(network) {
            network_destroy(network.socket);
            instance_destroy();
        }
    }
    show_debug_message("Server disconnected.");
    room_goto(rm_auth);
}



